@page "/TopList"
@using Core.Services.Movies.Models
@using Core.Services.Movies
@inject IMovieService MovieService
@if(_movieModel != null)
{
<TopListItemView Content="@_movieModel" />
}


@code {
    private MovieModel _movieModel;

    protected override async Task OnInitializedAsync()
    {
        _movieModel = await MovieService.GetMovieDetails("tt0120737");
    }
}
