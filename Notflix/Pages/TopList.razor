@page "/TopList"
@using Core.Services.Movies.Models
@using Core.Services.Movies
@inject IMovieService MovieService
@if(movieModal != null)
{
<TopListItemView Content="@movieModal" />
}


@code {
    private MovieModal movieModal;

    protected override async Task OnInitializedAsync()
    {
        movieModal = await MovieService.GetMovieDetails("tt0120737");
    }
}
